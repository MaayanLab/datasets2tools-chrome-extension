{
	"name": "Datasets2Tools",
	"version": "0.1",
	"description": "Ma'ayan Lab",
	
	"icons":{
		"128": "icon_128.png",
		"720": "icon_720.png"
	},
	
	"browser_action": {
		"default_icon": "icon_720.png"
	},
		
	"content_scripts": [
		{
			"matches": [
				"https://datamed.org/search.php?*",
				"https://www.ncbi.nlm.nih.gov/gds/?term=*",
				"http://lincsportal.ccs.miami.edu/datasets/*"
			],
			"css": [
				"css/style.css",
				"css/image.css",
				"css/button.css",
				"css/tooltip.css",
				"css/dropdown.css"
			],
			"js": [
				"jquery.js",
				"download.js",
				"datasets2tools.js"
			]
		}
	],
	
	"web_accessible_resources": [
		"icon_128.png",
		"icon_720.png"
	],

	"manifest_version": 2
}